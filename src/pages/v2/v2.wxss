/* pages/v2/v2.wxss */
.vessel {
    position: fixed;
    width: 100%;
    height: 100%;
    background: #FFF5EE;
    -webkit-overflow-scrolling: touch;
}

.box {
    width: 750rpx;
    height: 450rpx;
    position: relative;
    background: #FFF5EE;
    margin: 20% auto 0;
  }
  
